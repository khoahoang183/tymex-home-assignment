<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_home"
    app:startDestination="@id/homeUserFragment">

    <fragment
        android:id="@+id/homeUserFragment"
        android:name="com.khoahoang183.basesource.features.home.HomeFragment"
        tools:layout="@layout/fragment_home">

        <action
            app:destination="@+id/nav_graph_detail_user"
            android:id="@+id/action_home_to_detail_user">
            <argument
                android:name="userModel"
                app:nullable="true"
                app:argType="com.khoahoang183.model.features.GithubUserModel"/>

        </action>
    </fragment>

    <include app:graph="@navigation/nav_graph_detail_user"/>

</navigation>